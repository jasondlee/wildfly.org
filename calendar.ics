---
layout: none
---
BEGIN:VCALENDAR
PRODID:-//wildfly.org/WildFly Calendar V1.1//EN
URL: https://wildfly.org/calendar.ics
NAME: "WildFly Development Calendar"
REFRESH-INTERVAL;VALUE=DURATION:PT12H
VERSION:2.0
BEGIN:VTIMEZONE
TZID:America/New_York
BEGIN:DAYLIGHT
TZOFFSETFROM:-0500
TZOFFSETTO:-0400
TZNAME:CDT
DTSTART:19700308T020000
RRULE:FREQ=YEARLY;BYMONTH=3;BYDAY=2SU
END:DAYLIGHT
BEGIN:STANDARD
TZOFFSETFROM:-0600
TZOFFSETTO:-0500
TZNAME:CST
DTSTART:19701101T020000
RRULE:FREQ=YEARLY;BYMONTH=11;BYDAY=1SU
END:STANDARD
END:VTIMEZONE
METHOD:PUBLISH
{% for entry in site.data.schedule %}BEGIN:VEVENT
UID:{{ entry.date | date: "%Y%m%d" }}
LOCATION:https://wildfly.org
SUMMARY:{{ entry.description | strip }}
DESCRIPTION:{{ entry.comments | strip }}
CLASS:PUBLIC
DTSTART:{{ entry.date | date: "%Y%m%d" }}{% if entry.start_time %}{{ entry.start_time | date: "T%H%M%S" }}{% endif %}
DTEND:{{ entry.date | date: "%Y%m%d" }}{% if entry.end_time %}{{ entry.end_time |  date: "T%H%M%S" }}{% endif %}
DTSTAMP:{{ site.time | date: "%Y%m%dT%H%M%SZ" }}
END:VEVENT
{% endfor %}END:VCALENDAR
